<?xml version="1.0" encoding="UTF-8" ?>
<EditorStyle name="MonokaiStyle" _description="Monokai, Textmate-style theme">

  <Color name="string_main_color" value="#E6DB74" />
  <Color name="comment_main_color" value="#74705E" />
  <Color name="constant_main_color" value="#AE81FF" />  
  <Color name="main_red_color" value="#F92570" />  
  <Color name="main_lightblue_color" value="#61D5ED" />
  <Color name="main_green_color" value="#A4E22E" />
  <Color name="bg_dark_brownish" value="#3B3A32" />
  <Color name="error_bg" value="#D40000" />
  <Color name="text_fg" value="#F8F8F2" />

  <Style name="comment" color="#74705E">
	<Style name="tag" color="#74705E" />
	<Style name="line" color="#74705E" />
	<Style name="tag.line" color="#74705E" />
	<Style name="block" color="#74705E" />
	<Style name="tag.block" color="#74705E" />
	<Style name="doc" color="#339900" />
	<Style name="tag.doc" color="#339900" />
	<Style name="keyword" color="#339900" weight="Bold" />
	<Style name="keyword.todo" color="#339900" weight="Italic" />
  </Style>

  <Style name="text" color="text_fg" bgColor="#272822">
  	<Style name="background.readonly" color="#F9F9F8" />
  	<Style name="punctuation" color="main_red_color" />
  	<Style name="preprocessor" color="#336699" />
  	<Style name="preprocessor.keyword" color="#336699" weight="Bold" />
  	<Style name="markup" color="main_green_color" />
  	<Style name="markup.tag" color="main_lightblue_color" />
	<Style name="selection" color="#49483E" bgColor="#000000" />
	<Style name="selection.inactive" color="#FDD99B" bgColor="#A9A9A9" />
	<Style name="background.searchresult" color="#FFFF00" />
	<Style name="background.searchresult-main" color="#F3DD48" />
	<Style name="link" color="#0000FF" />
  </Style>

  <Style name="string" color="string_main_color">
	<Style name="single" color="string_main_color" />
	<Style name="double" color="string_main_color" />
	<Style name="other" color="string_main_color" />
  </Style>

  <Style name="linenumber" color="#BCBCBC" bgColor="bg_dark_brownish" >
  	<Style name="highlight" color="main_green_color" />
  </Style>

  <Style name="iconbar" color="bg_dark_brownish">
  	<Style name="separator" color="bg_dark_brownish" />
  </Style>

  <Style name="fold" color="#465457">
  	<Style name="highlight" color="#565248" />
  	<Style name="togglemarker" color="#465457" />
  </Style>

  <Style name="marker" color="#F92570">
  	<Style name="line.dirty" color="#F92570" />
  	<Style name="line.changed" color="main_green_color" />
  	<Style name="line" color="#3E3D31" />
	<Style name="ruler" color="#B3DEFD" />
	<Style name="whitespace" color="#D9BB7A" />
	<Style name="invalidline" color="error_bg" />
	<Style name="breakpoint" color="#FFFFFF" bgColor="#7D0000" />
	<Style name="breakpoint.color1" color="#FFFFFF" />
	<Style name="breakpoint.color2" color="#7D0000" />
	<Style name="breakpoint.disabled.background" color="#EDDCDC" />
	<Style name="debug.currentline" color="#000000" bgColor="#FEFF00" />
	<Style name="debug.currentline.color1" color="#FEFF00" />
	<Style name="debug.currentline.color2" color="#FEFECC" />
	<Style name="debug.currentline.border" color="#656600" />
	<Style name="debug.stackline" color="#000000" bgColor="#80FF80" />
	<Style name="debug.stackline.color1" color="#80FF80" />
	<Style name="debug.stackline.color2" color="#CCFECC" />
	<Style name="debug.stackline.border" color="#336633" />
	<Style name="breakpoint.invalid.background" color="#EDDCDC" />
	<Style name="breakpoint.invalid.color1" color="#EDDCDC" />
	<Style name="breakpoint.invalid.border" color="#7D0000" />
	<Style name="bracket" color="#6699CC" bgColor="#AACCEE" />
	<Style name="bookmark.color1" color="#FDD99B" />
	<Style name="bookmark.color2" color="#816647" />
	<Style name="underline.error" color="#FF0000" />
	<Style name="underline.warning" color="#FFA500" />
	<Style name="underline.suggestion" color="#8FC68F" />
	<Style name="underline.hint" color="#8F8FC6" />
	<Style name="template.primary_template" color="#015A01" bgColor="#98FC66" />
	<Style name="template.primary_highlighted_template" color="#015A01" bgColor="#CCFF99" />
	<Style name="template.secondary_template" color="#FFFFFF" bgColor="#FFFFFF" />
	<Style name="template.secondary_highlighted_template" color="#D9BB7A" bgColor="#FDD99B" />
	<Style name="whitespace.eol" color="#D9BB7A" />
	<Style name="breakpoint.invalid" color="#FFFFFF" bgColor="#7D0000" />
  </Style>

  <Style name="diff" color="#008A8B">
	<Style name="line-added" color="#008A8B" />
	<Style name="line-removed" color="#6A59CD" />
	<Style name="line-changed" color="#336699" />
	<Style name="header" color="#008000" weight="Bold" />
	<Style name="header-seperator" color="#0000FF" />
	<Style name="header-oldfile" color="#008000" weight="Bold" />
	<Style name="header-newfile" color="#008000" weight="Bold" />
	<Style name="location" color="#FB8B00" />
  </Style>

  <Style name="bubble" color="#000000" bgColor="#F3EEDA">
	<Style name="warning" color="#000000" bgColor="#F3EEDA" />
	<Style name="warning.text" color="#000000" />
	<Style name="error" color="#FFFFFF" bgColor="#7D0000" />
	<Style name="error.text" color="#000000" />
  </Style>

  <Style name="template" color="#000000">
	<Style name="tag" color="#FF00FF" />
	<Style name="directive" color="#FF00FF" />
  </Style>

  <Style name="constant" color="constant_main_color">
	<Style name="digit" color="constant_main_color" />
	<Style name="language" color="constant_main_color" />
	<Style name="language.void" color="main_lightblue_color" weight="Bold" />
  </Style>

  <Style name="keyword" color="main_red_color" weight="Bold">
	<Style name="access" color="main_green_color" weight="Bold" />
	<Style name="namespace" color="main_red_color" weight="Bold" />
	<Style name="property" color="main_green_color" weight="Bold" />
	<Style name="modifier" color="main_red_color" weight="Bold" />
	<Style name="type" color="main_lightblue_color" weight="Bold" />
	<Style name="declaration" color="main_lightblue_color" weight="Bold" />
	<Style name="operator" color="main_red_color" weight="Bold" />
	<Style name="operator.declaration" color="main_lightblue_color" weight="Bold" />
	<Style name="selection" color="main_red_color" weight="Bold" />
	<Style name="iteration" color="main_red_color" weight="Bold" />
	<Style name="jump" color="main_red_color" weight="Bold" />
	<Style name="context" color="main_red_color" weight="Bold" />
	<Style name="exceptions" color="main_red_color" weight="Bold" />
	<Style name="parameter" color="main_lightblue_color" weight="Bold" />
	<Style name="misc" color="#FB8700" weight="Bold" />
	<Style name="semantic.type" color="main_green_color" weight="Bold" />
	<Style name="semantic.field" color="main_green_color" weight="Italic" />
	<Style name="semantic.error" color="text_fg" bgColor="error_bg" weight="Italic" />
  </Style>
</EditorStyle>